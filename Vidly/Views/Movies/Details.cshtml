@model Vidly.Models.Movie

@{
    ViewBag.Title = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Details</h2>
<h2>@Model.Name</h2>
<ul>
    <li>Genre: @Model.Genre.Name</li>
    <li>Release Date: @Model.Release_Date.ToShortDateString()</li>
    <li>Date Added: @Model.Date_Add.ToShortDateString()</li>
    <li>Number in Stock: @Model.Stock</li>
</ul>

